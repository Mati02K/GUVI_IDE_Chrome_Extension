{
    "manifest_version" : 3,
    "name" : "Guvi IDE",
    "version" : "1.0",
    "description" : "Run your code from anywhere on the webpage !!!",
    "icons" : {
        "128" : "icons/icon128.png",
        "48" : "icons/icon48.png",
        "16" : "icons/icon16.png"
    },
    "action" : {
        "default_icon" : "icons/icon16.png",
        "default_popup" : "html/popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://ajaxorg.github.io/;"
    },
    "background" : {
        "service_worker": "js/eventPage.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/vendor/jquery-3.6.0.min.js", "js/content.js"]
        }
    ],
    "permissions" : [
        "storage",
        "notifications",
        "contextMenus",
        "activeTab"
    ]
}